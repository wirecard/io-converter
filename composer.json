{
  "name": "wirecard/iso-wppv2-converter",
  "description": "Converts country-language codes to WPPv2 supported language codes",
  "minimum-stability": "stable",
  "license": "GPL-3.0-only",
  "type": "library",
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "upload-coverage": "codacycoverage clover build/coverage/xml",
    "test": "phpunit --colors=always",
    "test-coverage": "phpunit --coverage-clover build/coverage/xml"
  },
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "squizlabs/php_codesniffer": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "Wirecard\\IsoToWppvTo\\": "src/"
    }
  }
}